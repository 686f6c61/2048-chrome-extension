{
  "manifest_version": 3,
  "name": "2048 Game",
  "description": "Play 2048 within your browser as a Chrome extension popup.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "2048"
  },
  "icons": {
    "16": "icons/icono32.png",
    "32": "icons/icono32.png",
    "48": "icons/icono48.png",
    "128": "icons/icono128.png"
  }
}
